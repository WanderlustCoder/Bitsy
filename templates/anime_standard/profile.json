{
  "name": "anime_standard",
  "description": "Standard anime style portrait",
  "canvas_size": [64, 96],
  "proportions": {
    "head_y": 0.06,
    "head_height_ratio": 0.45,
    "face_width_ratio": 0.50,
    "eye_y": 0.45,
    "eyebrow_y": 0.38,
    "nose_y": 0.60,
    "mouth_y": 0.70,
    "eye_spacing": 0.22
  },
  "templates": {
    "faces": ["oval"],
    "eyes": ["large"],
    "noses": ["dot"],
    "mouths": ["neutral"],
    "hair_back": ["wavy_back"],
    "hair_front": ["wavy_front"],
    "bodies": ["neutral", "holding"],
    "accessories": ["glasses_round"]
  },
  "coloring": {
    "use_hue_shift": true,
    "shadow_hue_shift": -15,
    "rim_light_color": [150, 170, 255],
    "rim_light_intensity": 0.9
  },
  "post_processing": {
    "outline": "thin",
    "outline_color": [40, 30, 50],
    "selective_aa": true
  }
}
